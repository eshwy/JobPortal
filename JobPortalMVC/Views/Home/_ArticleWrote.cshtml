@model IEnumerable<JopPortalMVC.Models.ArticleTbl>

    
    <div class="d-flex align-items-center justify-content-center">
        <h3>Articles written</h3>
    </div>
    <div class="row">
        <div class="col-md-7">
            <h5>Article Name</h5>
        </div>
    </div>
    <div class="row">
        @foreach (var item in Model)
        {
        <div class="col-md-7">
            @Html.DisplayFor(modelItem => item.Title)
        </div>
        <div class="col-md-4">
            @Html.ActionLink("View", "ViewArticle", "Home", new { artileId = item.RowId}, new { @class = "btn btn-outline-primary" })
        </div>
            
        }
    </div>